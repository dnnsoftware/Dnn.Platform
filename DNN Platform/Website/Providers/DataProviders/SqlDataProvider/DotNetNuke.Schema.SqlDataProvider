        L.*,
        PL.PortalId,
        PL.IsPublished
    FROM   {databaseOwner}{objectQualifier}Languages L
        INNER JOIN {databaseOwner}{objectQualifier}PortalLanguages PL On L.LanguageID = PL.LanguageID
    WHERE PL.PortalID = @PortalID
