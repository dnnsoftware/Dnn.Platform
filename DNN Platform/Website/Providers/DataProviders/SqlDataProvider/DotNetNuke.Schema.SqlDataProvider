create procedure {databaseOwner}[{objectQualifier}DeleteUrl]

@PortalID     int,
@Url          nvarchar(255)

as

delete
from   {databaseOwner}{objectQualifier}Urls
where  PortalID = @PortalID
and    Url = @Url
