CREATE PROCEDURE {databaseOwner}[{objectQualifier}GetFolderMappings]
	@PortalID int
AS
BEGIN
	SELECT *
	FROM {databaseOwner}[{objectQualifier}FolderMappings]
	WHERE ISNULL(PortalID, -1) = ISNULL(@PortalID, -1)
	ORDER BY Priority
END
