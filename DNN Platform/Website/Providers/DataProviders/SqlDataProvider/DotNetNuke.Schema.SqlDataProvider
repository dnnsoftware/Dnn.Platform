CREATE procedure {databaseOwner}[{objectQualifier}GetPortalPermissionsByPortal]
	
	@PortalID int

AS

	IF @PortalID is not null
		BEGIN 
			SELECT *
				FROM {databaseOwner}{objectQualifier}vw_PortalPermissions
				WHERE PortalID = @PortalID
		END
	ELSE
		BEGIN
			SELECT *
				FROM {databaseOwner}{objectQualifier}vw_PortalPermissions
				WHERE PortalID IS NULL 
		END

