CREATE PROCEDURE {databaseOwner}[{objectQualifier}GetFileByUniqueID]
    @UniqueID   uniqueidentifier
AS
	SELECT	* 
	FROM	{databaseOwner}{objectQualifier}Files
	WHERE	UniqueID = @UniqueID
