CREATE PROCEDURE {databaseOwner}[{objectQualifier}GetFileVersion] 
	@FileId int,
	@Version int
AS
BEGIN
	SELECT 
	   [FileId]
      ,[Version]
      ,[FileName]
      ,[Extension]
      ,[Size]
      ,[Width]
      ,[Height]
      ,[ContentType]
      ,[CreatedByUserID]
      ,[CreatedOnDate]
      ,[LastModifiedByUserID]
      ,[LastModifiedOnDate]
      ,[SHA1Hash]
	FROM {databaseOwner}{objectQualifier}FileVersions fv
	WHERE FileId = @FileId
	  AND Version = @Version
END
GO
