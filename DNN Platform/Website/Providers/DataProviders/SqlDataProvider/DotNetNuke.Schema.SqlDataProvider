create procedure {databaseOwner}[{objectQualifier}FindDatabaseVersion]

@Major  int,
@Minor  int,
@Build  int

as

select 1
from   {databaseOwner}{objectQualifier}Version
where  Major = @Major
and    Minor = @Minor
and    Build = @Build
