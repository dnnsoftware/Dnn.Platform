CREATE PROCEDURE {databaseOwner}[{objectQualifier}GetFileVersionContent]

	@FileId		int,
	@Version	int

AS
	SELECT Content
	FROM {databaseOwner}[{objectQualifier}FileVersions]
	WHERE FileId = @FileId
		AND Version = @Version
GO
