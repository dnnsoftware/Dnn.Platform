CREATE procedure {databaseOwner}[{objectQualifier}GetTabPermissionsByPortal]
	
	@PortalID int

AS

	IF @portalid is not null
		BEGIN 
			SELECT *
				FROM {databaseOwner}{objectQualifier}vw_TabPermissions
				WHERE PortalID = @PortalID
		END
	ELSE
		BEGIN
			SELECT *
				FROM {databaseOwner}{objectQualifier}vw_TabPermissions
				WHERE PortalID IS NULL 
		END
GO
