	@UserID int,
	@PortalID INT
AS
BEGIN
	SELECT TOP 1 *	
	FROM {databaseOwner}[{objectQualifier}CoreMessaging_Messages]
	WHERE SenderUserID = @UserID	
	AND PortalID=@PortalID
	AND NotificationTypeID IS NULL
	ORDER BY MessageID DESC
END
