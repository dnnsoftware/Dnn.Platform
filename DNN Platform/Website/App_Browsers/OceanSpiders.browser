	<browser id="MozillaSpider" parentID="Mozilla">
		<identification>
			<userAgent match="(?i-s:crawler|bot|spider|larbin|search|indexer|archiver|Nutch)" />
		</identification>

		<capture>
			<!--
		Some spiders use the Mozilla/5.0 prefix but don't use the '(compatible;' tag.  This causes the major and minor versions to misidentified
		-->
