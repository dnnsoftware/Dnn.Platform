<configuration>
  <nodes configfile="web.config">
      <node path="/configuration/appSettings/add[@key='AuthorizationServerPrivateKey']" action="updateattribute"  name="key" value="" />
	    <node path="/configuration/appSettings/add[@key='ResourceServerDecryptionKey']" action="updateattribute"  name="key" value="" />
		  <node path="/configuration/appSettings/add[@key='AuthorizationServerVerificationKey']" action="updateattribute"  name="key" value="" />
		  
		   <node path="/configuration/configSections/sectionGroup[@name='dotNetOpenAuth']" action="update" key="name" collision="ignore">
      <sectionGroup name="dotNetOpenAuth" type="DotNetOpenAuth.Configuration.DotNetOpenAuthSection, DotNetOpenAuth.Core">
      <sectionGroup name="oauth2" type="DotNetOpenAuth.Configuration.OAuth2SectionGroup, DotNetOpenAuth.OAuth2">
        <section name="authorizationServer" type="DotNetOpenAuth.Configuration.OAuth2AuthorizationServerSection, DotNetOpenAuth.OAuth2.AuthorizationServer" requirePermission="false" allowLocation="true" />
      </sectionGroup>
      <section name="messaging" type="DotNetOpenAuth.Configuration.MessagingElement, DotNetOpenAuth.Core" requirePermission="false" allowLocation="true" />
      <section name="reporting" type="DotNetOpenAuth.Configuration.ReportingElement, DotNetOpenAuth.Core" requirePermission="false" allowLocation="true" />
    </sectionGroup>
    </node>
	
  </nodes>
</configuration>