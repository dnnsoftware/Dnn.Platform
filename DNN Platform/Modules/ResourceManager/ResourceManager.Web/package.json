    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-polyfill": "6.26.0",
    "copy-to-clipboard": "^3.0.5",
    "css-loader": "2.1.1",
    "eslint": "5.9.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-spellcheck": "0.0.11",
