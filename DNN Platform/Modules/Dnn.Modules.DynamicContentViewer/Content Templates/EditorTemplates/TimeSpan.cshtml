@using DotNetNuke.Web.Client.ClientResourceManagement
@model TimeSpan

@{
    ClientResourceManager.RegisterStyleSheet(Dnn.DnnPage, "~/Resources/Shared/Components/TimePicker/Themes/jquery-ui.css");
    ClientResourceManager.RegisterStyleSheet(Dnn.DnnPage, "~/Resources/Shared/Components/TimePicker/Themes/jquery.ui.theme.css");
    ClientResourceManager.RegisterScript(Dnn.DnnPage, "~/Resources/Shared/Components/TimePicker/jquery.timepicker.js");
    ClientResourceManager.RegisterStyleSheet(Dnn.DnnPage, "~/Resources/Shared/Components/TimePicker/jquery.timepicker.css");
}

@Html.TextBox(ViewData["FieldName"] as string, Model)

<script language="javascript" type="text/javascript">
    jQuery(document).ready(function($) {
        $(".dccEditContent #@ViewData["FieldName"]").timepicker();
    });
</script>