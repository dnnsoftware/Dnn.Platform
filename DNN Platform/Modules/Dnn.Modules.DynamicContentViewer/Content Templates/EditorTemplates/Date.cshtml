@using DotNetNuke.Web.Client.ClientResourceManagement
@model DateTime

@{
    ClientResourceManager.RegisterStyleSheet(Dnn.DnnPage, "~/Resources/Shared/Components/TimePicker/Themes/jquery-ui.css");
    ClientResourceManager.RegisterStyleSheet(Dnn.DnnPage, "~/Resources/Shared/Components/TimePicker/Themes/jquery.ui.theme.css");
}

@Html.TextBox(ViewData["FieldName"] as string, Model.ToString("d"))

<script language="javascript" type="text/javascript">
    jQuery(document).ready(function($) {
        $(".dccEditContent #@ViewData["FieldName"]").datepicker();
    });
</script>