            <install>
              <configuration>
                <nodes>
                  <node path="/configuration/dotnetnuke/navigationControl/providers" action="update" key="name" collision="overwrite">
                    <add name="NewDDRMenuNavigationProvider" type="DotNetNuke.Web.NewDDRMenu.NewDDRMenuNavigationProvider, DotNetNuke.Web.NewDDRMenu" />
                  </node>
                </nodes>
              </configuration>
            </install>
            <uninstall>
              <configuration>
                <nodes>
                  <node path="/configuration/dotnetnuke/navigationControl/providers/add[@name='NewDDRMenuNavigationProvider']" action="remove" />
                </nodes>
              </configuration>
            </uninstall>
