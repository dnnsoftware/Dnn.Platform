						</file>
						<file>
							<name>RedirectionSettingsView.ascx</name>
							<sourceFileName>Views\RedirectionSettingsView.ascx</sourceFileName>
						</file>
						<file>
							<name>SimpleSettingsView.ascx</name>
							<sourceFileName>Views\SimpleSettingsView.ascx</sourceFileName>
						</file>
