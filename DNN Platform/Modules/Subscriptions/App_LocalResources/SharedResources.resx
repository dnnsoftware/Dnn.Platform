  <data name="Template_Email.Text" xml:space="preserve">
    <value>&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"&gt;
&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;
&lt;head&gt;
    &lt;meta http-equiv="Content-Type" content="text/html; charset=UTF-8" /&gt;
    &lt;style type="text/css"&gt;
            /* Reset Styles */
        html, body{height:100%;width:100%;}
        body{
            margin:0; padding:0;
            font-family:sans-serif;
            font-size:0.83em;
            color:#333;
        }
        img{border:0; height:auto; line-height:100%; outline:none; text-decoration:none;}
        table td{border-collapse:collapse;}
        p{margin: 0 0 1.6em 0;}
        a{color:#417CD3;}
        #backgroundTable{height:100% !important; margin:0; padding:0; width:100% !important;}
        body{ background-color:#fafafa; }        

        #templateContainer{
            border: 1px solid #eeeeee;
            box-shadow: 0px 0px 3px 0px #eee;
        }

            /* Pre Header Styles */
        .preheaderContent{
            padding:15px 0 5px 0;
            font-size:0.9em;
            color:#999;
        }
        .headerContent {background-color:#fcb52f;}
        .headerContent a{display:block;}
        .headerContent img{ margin-bottom:0;}

            /* Sub Header Styles */
        #templateSubHeader{ color:#ffffff; }
        #templateSubHeader td{ padding:5px 20px; }

            /* Template Footer Styles */
        .footerContent table td{
            padding-top:20px;
            border-top:1px solid #ddd;
            font-size:0.8em;
        }

            /* Body Template Footer Styles */
        #bodyTemplateFooter{
            padding:15px 0 5px 0;
            font-size:0.9em;
            color:#999;
        }
    &lt;/style&gt;
&lt;/head&gt;

&lt;body leftmargin="0" marginwidth="0" topmargin="0" marginheight="0" offset="0"&gt;
&lt;table cellpadding="0" cellspacing="0" border="0" width="100%" height="100%" id="backgroundTable"&gt;
    &lt;tr&gt;
        &lt;td align="center" valign="top"&gt;
            &lt;!-- // Begin Template Preheader \\ --&gt;
            &lt;table border="0" cellpadding="0" cellspacing="0" id="templatePreheader" width="600"&gt;
                &lt;tr align="right"&gt;
                    &lt;td valign="top" class="preheaderContent" style="border-top:5px solid #417CD3;"&gt;

                        &lt;!-- // Begin Module: Standard Preheader \ --&gt;
                        &lt;table border="0" cellpadding="0" cellspacing="0" &gt;
                            &lt;tr&gt;
                                &lt;td align="right"&gt;
                                    &lt;p&gt;Problem viewing email? &lt;a href="[NOTIFICATIONURL]"&gt;&lt;span&gt;Click here to view online.&lt;/span&gt;&lt;/a&gt;&lt;/p&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;!-- // End Module: Standard Preheader \ --&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
            &lt;/table&gt;
            &lt;!-- // End Template Preheader \\ --&gt;
            &lt;!-- // Begin Template body \\ --&gt;
            &lt;table border="0" cellpadding="0" cellspacing="0" width="600" id="templateContainer" style="border:1px solid #eeeeee; " &gt;
                &lt;tr &gt;
                    &lt;td align="center" valign="top"&gt;
                        &lt;!-- // Begin Template Header \\ --&gt;
                        &lt;table border="0" cellpadding="30" cellspacing="0" width="600" id="templateHeader"&gt;
                            &lt;tr&gt;
                                &lt;td class="headerContent"&gt;
                                    &lt;a href="[SITEURL]"&gt;
                                        &lt;!--&lt;img src="[LOGOURL]" style="max-width:600px;" id="headerImage campaign-icon" /&gt;--&gt;
                                        &lt;img src="[LOGOURL]" style="max-width:600px;" id="headerImage campaign-icon" width="200" /&gt;
                                    &lt;/a&gt;

                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;!-- // End Template Header \\ --&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;tr &gt;
                    &lt;td align="center" valign="top"&gt;
                        &lt;!-- // Begin Template Sub Header \\ --&gt;
                        &lt;table border="0" cellpadding="0" cellspacing="0" width="600" id="templateSubHeader"&gt;
                            &lt;tr&gt;
                                &lt;td class="subHeaderContent" bgcolor="#417CD3" width="66%" align="left"&gt;
                                    &lt;h2 style="color:#fff; font-weight:100; font-size:24px;"&gt;Notifications&lt;/h2&gt;
                                &lt;/td&gt;&lt;!--close subHeaderContent--&gt;
                                &lt;td width="33%" bgcolor="#417CD3" align="right"&gt;
                                    &lt;p style="color:#fff;"&gt;&lt;/p&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;!-- // End Template Sub Header \\ --&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;tr&gt;
                    &lt;td align="center" valign="top" bgcolor="#ffffff"&gt;
                        &lt;!-- // Begin Template Body \\ --&gt;
                        &lt;table border="0" cellpadding="0" cellspacing="0" width="600" id="templateBody"&gt;
                            &lt;tr&gt;
                                &lt;td valign="top" class="bodyContent"&gt;
                                    &lt;!-- // Begin Module: Standard Content \\ --&gt;
                                    &lt;table border="0" cellpadding="30" cellspacing="0" width="100%"&gt;
                                        &lt;tr&gt;
                                            &lt;td valign="top"&gt;
                                                [MESSAGEBODY]
                                            &lt;/td&gt;
                                        &lt;/tr&gt;
                                    &lt;/table&gt;
                                    &lt;!-- // End Module: Standard Content \\ --&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;!-- // End Template Body \\ --&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;tr&gt;
                    &lt;td align="center" valign="top"&gt;
                        &lt;table border="0" cellpadding="30" cellspacing="0" width="600" id="templateFooter"&gt;
                            &lt;tr&gt;
                                &lt;td valign="top" class="footerContent"&gt;
                                    &lt;table border="0" cellpadding="0" cellspacing="0" width="100%"&gt;
                                        &lt;tr&gt;
                                            &lt;td align="center"&gt;
                                                &lt;a href="[UNSUBSCRIBEURL]" &gt; Manage your Subscriptions&lt;/a&gt; &amp;nbsp; |  &amp;nbsp;
                                                &lt;a href="http://www.twitter.com/" &gt; Follow us on Twitter&lt;/a&gt; &amp;nbsp; |  &amp;nbsp;
                                                &lt;a href="http://www.facebook.com/" &gt; Become a fan on Facebook&lt;/a&gt;

                                            &lt;/td&gt;
                                        &lt;/tr&gt;
                                    &lt;/table&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
            &lt;/table&gt;&lt;!-- // End Template Body \\ --&gt;
            &lt;!-- // Begin Template footer \\ --&gt;
            &lt;table border="0" cellpadding="0" cellspacing="0" id="bodyTemplateFooter" width="600"&gt;
                &lt;tr&gt;
                    &lt;td valign="top" class="bodyFooterContent"&gt;

                        &lt;!-- // Begin Module: Standard Preheader \ --&gt;
                        &lt;table border="0" cellpadding="0" cellspacing="0" align="center" &gt;

                            &lt;tr&gt;
                                &lt;td align="center"&gt;
                                    &lt;p&gt;Copyright 2013 &lt;a href="[SITEURL]"&gt;Anova&lt;/a&gt; All rights reserved.&lt;/p&gt;
                                    &lt;p&gt;If you wish to no longer receive emails in the future, please &lt;a href="[UNSUBSCRIBEURL]"&gt;unsubscribe&lt;/a&gt; here.&lt;/p&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                        &lt;!-- // End Module: Standard Preheader \ --&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
            &lt;/table&gt;
            &lt;!-- // End Template footer \\ --&gt;
        &lt;/td&gt;
    &lt;/tr&gt;
&lt;/table&gt;&lt;!-- wrapper table --&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
  </data>
  <data name="Template_Item.Text" xml:space="preserve">
    <value>&lt;div style="width:100%;"&gt;&lt;h5&gt;[TITLE]&lt;/h5&gt;&lt;table width="600" border="0" cellpadding="0" cellspacing="0" class="contenttable" style="background:#fafafa;"&gt;
    &lt;tr&gt;
        &lt;td&gt;
            &lt;table width="80" border="0" cellspacing="0" cellpadding="8" align="left"&gt;
                &lt;tr&gt;
                    &lt;td&gt;&lt;img src="[PROFILEPICURL]" /&gt;&lt;/td&gt;
                &lt;/tr&gt;
            &lt;/table&gt;
            &lt;table width="350" border="0" cellspacing="0" cellpadding="8" align="left"&gt;
                &lt;tr&gt;
                    &lt;td&gt;[CONTENT]&lt;/td&gt;
                &lt;/tr&gt;
            &lt;/table&gt;
        &lt;/td&gt;
    &lt;/tr&gt;
&lt;/table&gt;&lt;/div&gt;</value>
  </data>
