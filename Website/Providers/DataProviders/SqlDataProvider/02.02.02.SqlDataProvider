	(
	VendorId
	) ON [PRIMARY]
GO

ALTER TABLE {databaseOwner}{objectQualifier}Banners WITH NOCHECK ADD CONSTRAINT
	FK_{objectQualifier}Banner_{objectQualifier}Vendor FOREIGN KEY
	(
	VendorId
	) REFERENCES {databaseOwner}{objectQualifier}Vendors
	(
	VendorId
	) ON DELETE CASCADE
	 NOT FOR REPLICATION

GO

