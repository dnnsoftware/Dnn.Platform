	FK_{objectQualifier}PortalAlias_{objectQualifier}Portals FOREIGN KEY
	(
	PortalID
	) REFERENCES {databaseOwner}{objectQualifier}Portals
	(
	PortalID
	) ON DELETE CASCADE
	
GO

