			[SettingValue] [nvarchar](2000) NOT NULL,
			CONSTRAINT [PK_{objectQualifier}ExtensionUrlProviderSetting] PRIMARY KEY CLUSTERED 
			(
				[ExtensionUrlProviderID] ASC,
				[PortalID] ASC,
				[SettingName] ASC
			)
		)
	END
GO

