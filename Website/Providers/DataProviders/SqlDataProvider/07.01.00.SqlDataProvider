BEGIN

INSERT INTO {databaseOwner}{objectQualifier}CoreMessaging_NotificationTypes(
	[Name]
      ,[Description]
      ,[CreatedByUserID]
      ,[CreatedOnDate]
      ,[LastModifiedByUserID]
      ,[LastModifiedOnDate]
) VALUES (
	'ContentWorkflowNotification',
	'Content Workflow Notification',
	-1,
	GETDATE(),
	-1,
	GETDATE()
)
END

